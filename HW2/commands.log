sudo yum install mdadm gdisk -y
sudo mdadm --zero-superblock /dev/sd{b,c,d,e,f}
sudo mdadm --create /dev/md0 -l 6 -n 4 -x 1 /dev/sd{b,c,d,e,f}
sudo mdadm -f /dev/md0 /dev/sde
sudo mdadm -f /dev/md0 /dev/sdb
sudo mdadm -r /dev/md0 /dev/sde
sudo mdadm -r /dev/md0 /dev/sdb
sudo mdadm -a /dev/md0 /dev/sde
sudo mdadm -a /dev/md0 /dev/sdb
sudo gdisk /dev/md0
